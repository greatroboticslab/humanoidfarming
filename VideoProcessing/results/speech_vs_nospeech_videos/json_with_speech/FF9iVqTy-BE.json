    {
      "id": 11,
      "start": 111.3,
      "end": 115.3,
      "text": "\u0643\u0646\u0627 \u0645\u062b\u0644\u0627\u0646 \u0646\u0635\u0642 \u0646\u062d\u0646 \u0639\u0627\u0644\u0633\u0639\u0631 \u0639\u0644\u0649 5 \u062a\u064a\u0627\u0645 10 \u0633\u0627\u0639\u0629"
    },
    {
      "id": 12,
      "start": 115.3,
      "end": 118.3,
      "text": "\u0646\u0628\u0644\u0634 6 \u0633\u0627\u0639\u0629 2 \u0639\u062f\u064a\u0646 8 \u0633\u0627\u0639\u0629"
    },
    {
      "id": 13,
      "start": 118.3,
      "end": 120.3,
      "text": "2-3 \u0639\u062f\u064a\u0646 \u0646\u0635\u0642 \u0627\u0644\u0644\u064a 14 \u0633\u0627\u0639\u0629"
    },
    {
      "id": 14,
      "start": 120.3,
      "end": 125.3,
      "text": "\u0647\u0644\u0623 \u0647\u0646\u0628\u063a\u064a\u0631 \u0627\u0644\u0648\u0627\u0636\u0639 \u0648\u0646\u0643\u0648\u0646 \u062a\u062c\u064a \u064a\u0628\u0639\u0637\u0648\u0646\u0646\u0627 \u0627\u0644\u0647\u0646\u0633\u064a\u0646 \u0627\u0644\u0645\u0634\u0631\u0648\u0639"
    },
    {
      "id": 15,
      "start": 125.3,
      "end": 128.3,
      "text": "\u064a\u0643\u0634\u0641 \u0639\u064a \u064a\u0643\u0634\u0641 \u0627\u0644\u0644\u064a \u062a\u0639\u0637\u064a \u0647\u0627\u0644\u0623\u062f\u0648 \u0647\u0627\u0644\u0623\u062f\u0648"
    },
    {
      "id": 16,
      "start": 128.3,
      "end": 131.3,
      "text": "\u0646\u062d\u0646 \u0643\u0646\u0627 \u0646\u062d\u0637 \u0639\u0634\u0648\u0627\u0626\u064a \u0648\u0631\u0637\u0646\u064a\u0647\u0627 \u0639\u0646 \u0623\u0647\u0644\u0646\u0627 \u0647\u0627 \u0627\u0644\u0645\u0635\u062d\u0629"
    },
    {
      "id": 17,
      "start": 131.3,
      "end": 134.3,
      "text": "\u0643\u0646\u0627 \u0646\u062d\u0637 \u0639\u0634\u0648\u0627\u0626\u064a \u0645\u062a\u064a\u0646 \u0643\u064a\u0644\u0648 \u0645\u0631\u0643\u0628 \u0625\u0646\u0628\u0643\u0627\u0631"
    },
    {
      "id": 18,
      "start": 134.3,
      "end": 136.3,
      "text": "100 \u0643\u064a\u0644\u0648 \u0623\u0632\u0648\u0647\u062a"
    },
    {
      "id": 19,
      "start": 136.3,
      "end": 142.3,
      "text": "\u0648\u0634\u0648 \u0643\u0627\u0646 \u062f\u0627\u0631\u064a\u0634 \u0628\u0630\u0627\u0643 \u0627\u0644\u0648\u0642\u062a \u0646\u062d\u0637\u0648\u0627 \u0646\u062d\u0637 \u0642\u062f\u0648\u064a \u0648\u0646\u062d\u0637 \u0627\u0633\u0645\u062f\u064a \u0648\u0646\u0642\u0627\u062a\u0631"
    },
    {
      "id": 20,
      "start": 142.3,
      "end": 145.3,
      "text": "\u0627\u0644\u0644\u064a \u0639\u0645 \u0646\u062d\u0637\u0648\u0627 \u0646\u062d\u0646\u0627 \u0628\u0627\u0644\u0623\u0631\u0636 \u0627\u0644\u0623\u0633\u0645\u062f\u064a \u0648\u0627\u0644\u0643\u0645\u064a\u0648"
    },
    {
      "id": 21,
      "start": 145.3,
      "end": 147.3,
      "text": "\u0648\u0647\u064a \u0641\u064a\u0647\u0627 \u0636\u0631\u0631 \u0639\u0644\u0649 \u0627\u0644\u0628\u0642\u0627\u0621"
