    {
      "id": 9,
      "start": 105.3,
      "end": 107.3,
      "text": "\u0641\u0647\u0645\u0648\u0646 \u0627\u0644\u0634\u0628\u0627\u0628\u064a \u0628\u0643\u0648\u0646 \u0631\u0637\u0648\u0628\u064a"
    },
    {
      "id": 10,
      "start": 107.3,
      "end": 111.3,
      "text": "\u0648\u0643\u0644 \u0623\u062f\u0627\u064a \u0641\u0627\u0643\u064a \u0648\u0646\u0628\u062a\u064a \u0643\u0645 \u062a\u0643\u0648\u0646 \u0637\u0627\u0644\u0628\u064a\u0646 \u0628\u064a\u0646\u064a \u0639\u0644\u0649 \u0645\u0646\u0638\u0631"
