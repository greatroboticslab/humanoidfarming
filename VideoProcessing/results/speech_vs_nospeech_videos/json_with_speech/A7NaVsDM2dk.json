      "text": "4.0 \u0688e."
    },
    {
      "id": 29,
      "start": 364.71,
      "end": 367.71,
      "text": "5.0 \u0688e."
    },
    {
      "id": 30,
      "start": 367.71,
      "end": 370.37,
      "text": "5.0 \u0688e."
    },
    {
      "id": 31,
      "start": 370.37,
      "end": 372.77,
      "text": "6.0 \u0688e."
    },
    {
      "id": 32,
      "start": 372.77,
      "end": 376.27,
      "text": "7.0 \u0688e."
    },
    {
      "id": 33,
      "start": 376.27,
      "end": 378.27,
      "text": "8.0 \u0688e."
    },
    {
      "id": 34,
      "start": 378.27,
      "end": 380.23,
      "text": "9.0 \u0688e."
    },
    {
      "id": 35,
      "start": 380.23,
      "end": 382.23,
      "text": "9.0 \u0688e."
    },
    {
      "id": 36,
      "start": 382.23,
